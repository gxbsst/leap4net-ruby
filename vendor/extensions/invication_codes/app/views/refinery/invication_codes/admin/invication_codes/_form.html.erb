<%= form_for [refinery, :invication_codes_admin, @invication_code] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @invication_code,
              :include_object_name => true %>

  
  <div class='field'>
    <%= f.label :begin_at -%>
    <%= f.datetime_select :begin_at  -%>

  </div>
  
  <div class='field'>
    <%= f.label :end_at -%>
    <%= f.datetime_select :end_at  -%>

  </div>
  
  <div class='field'>
    <%= f.label :code -%>
    <%= f.text_field :code  -%>

  </div>
  
  <div class='field'>
    <%= f.label :user_type -%>
    <%= f.text_field :user_type  -%>

  </div>
  
  <div class='field'>
    <%= f.label :user_id -%>
    <%= f.number_field :user_id  -%>

  </div>
  
  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.invication_codes.admin.invication_codes.invication_code'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @invication_code.code) %>
<% end -%>
